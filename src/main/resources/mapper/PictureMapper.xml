<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="main.biggreenbook.entity.dao.PictureMapper">
    <select id="getPictureBySid" parameterType="java.lang.String" resultType="main.biggreenbook.entity.pojo.Picture">
        select *
        from picture
        where sid = #{sid}
    </select>
    <select id="getPictureBySidIndex" resultType="main.biggreenbook.entity.pojo.Picture">
        select *
        from picture
        where sid = #{sid}
          and `index` = #{index}
    </select>
</mapper>
